generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Purchase {
  id     Int    @id @default(autoincrement())
  name   String
  mobile String
  userId Int
  Users  Users  @relation(fields: [userId], references: [id], onUpdate: Restrict)
}

model Users {
  id       Int        @id @default(autoincrement())
  username String
  password String
  Purchase Purchase[]
}
